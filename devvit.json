{
  "$schema": "https://developers.reddit.com/schema/config-file.v1.json",
  "name": "spottit-react2",
  "post": {
    "dir": "dist/client",
    "entrypoints": {
      "default": {
        "entry": "index.html"
      }
    }
  },
  "blocks": {
    "entry": "src/blocks/main.tsx"
  },
  "server": {
    "dir": "dist/server",
    "entry": "index.cjs"
  },
  "media": {
    "dir": "assets"
  },

  "triggers": {
    "onAppInstall": "/internal/on-app-install"
  },
  "dev": {
    "subreddit": "spottit_react2_dev"
  }
}
